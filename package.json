{
  "name": "api-mp3-to-itunes",
  "version": "0.0.1",
  "description": "The API of mp3-to-itunes.com",
  "main": "dist",
  "scripts": {
    "dev": "nodemon -w src --exec \"babel-node src --presets es2015,stage-0\"",
    "build": "babel src -s -D -d dist --presets es2015,stage-0",
    "start": "node dist",
    "prestart": "npm run -s build",
    "test": "eslint src"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 7,
      "sourceType": "module"
    },
    "env": {
      "node": true
    },
    "rules": {
      "no-console": 0,
      "no-unused-vars": 1
    }
  },
  "repository": "https://github.com/slorenzo/api-mp3-to-itunes",
  "author": "Sebastian Lorenzo <sebastianlorenzo@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "async-file": "^2.0.2",
    "axios": "^0.17.1",
    "body-parser": "^1.13.3",
    "compression": "^1.5.2",
    "cors": "^2.7.1",
    "dos-config": "^3.0.2",
    "express": "^4.13.3",
    "express-fileupload": "^0.3.0",
    "ffmetadata": "^1.4.0",
    "file-zip": "^1.0.1",
    "last-fm": "^5.1.2",
    "log4js": "^2.4.1",
    "morgan": "^1.8.0",
    "resource-router-middleware": "^0.6.0",
    "uuid": "^3.1.0",
    "zip-dir": "^1.0.2"
  },
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-core": "^6.9.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.5.0",
    "copy-dir": "^0.3.0",
    "eslint": "^3.1.1",
    "nodemon": "^1.9.2"
  }
}
